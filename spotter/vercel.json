{
  "version": 2,
  "builds": [
    {
      "src": "spotter/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "spotter/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "spotter.settings"
  }
}